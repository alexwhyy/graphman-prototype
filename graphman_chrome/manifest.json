{
    "manifest_version": 2,
    "name": "Desmos Collaboration",
    "description": "In development",
    "version": "1.0",
    
    "content_scripts": [
      {
        "matches": ["*://*.desmos.com/*"],
        "js": ["content.js"]
      }
    ],
    "permissions": [
      "*://*.desmos.com/*"
    ]   
}